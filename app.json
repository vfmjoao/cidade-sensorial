{
  "expo": {
    "name": "Cidade Sensorial",
    "slug": "cidade-sensorial",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cidadesensorial.app"
    },
    "android": {
      "package": "com.cidadesensorial.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
